{
  "name": "@lopopolo/www.yuankunandryan.com",
  "version": "0.1.0",
  "description": "Wedding website for Yuankun and Ryan",
  "keywords": [
    "javascript minification",
    "css minification",
    "asset caching",
    "build pipeline"
  ],
  "homepage": "https://www.yuankunandryan.com",
  "bugs": "https://github.com/lopopolo/www.yuankunandryan.com/issues",
  "license": "MIT",
  "author": "Ryan Lopopolo <rjl@hyperbo.la> (https://hyperbo.la/)",
  "contributors": [
    "Ryan Lopopolo <rjl@hyperbo.la> (https://hyperbo.la/)"
  ],
  "repository": "github:lopopolo/www.yuankunandryan.com",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "aws-sdk": "^2.706.0",
    "babel-loader": "^8.0.6",
    "cname-webpack-plugin": "^2.0.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.22.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jimp": "^0.13.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "responsive-loader": "^1.2.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jquery": true
    },
    "plugins": [
      "html",
      "import"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "settings": {
      "import/resolver": {
        "node": {},
        "webpack": {}
      }
    }
  },
  "scripts": {
    "build": "webpack -p --mode production",
    "dev": "webpack-dev-server --mode development",
    "dev:open": "webpack-dev-server --mode development --open",
    "fmt": "npx prettier --write '**/*'",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix ."
  }
}
